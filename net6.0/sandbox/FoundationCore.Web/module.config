<?xml version="1.0" encoding="utf-8"?>
<module>
  <dojo>
	<!-- This adds the FoundationCore template assembly to the "default module" -->
	<paths>
	  <add name="foundationcore" path="~/scripts/cms" />
	</paths>
  </dojo>
  <clientModule initializer="foundationcore/initializer">
	<moduleDependencies>
	  <add dependency="CMS" type="RunAfter" />
	</moduleDependencies>
  </clientModule>
</module>